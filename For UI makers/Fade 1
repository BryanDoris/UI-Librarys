	-- Scripts:

	local function RPHX_fake_script() -- Dragger.DraggingScript 
		local script = Instance.new('LocalScript', Dragger)

		local UserInputService = game:GetService("UserInputService")

		local gui = script.Parent

		local dragging
		local dragInput
		local dragStart
		local startPos

		local function update(input)
			local delta = input.Position - dragStart
			gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		end

		gui.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
				dragging = true
				dragStart = input.Position
				startPos = gui.Position

				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end)
			end
		end)

		gui.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
				dragInput = input
			end
		end)

		UserInputService.InputChanged:Connect(function(input)
			if input == dragInput and dragging then
				update(input)
			end
		end)
	end
	coroutine.wrap(RPHX_fake_script)()
	local function BFDW_fake_script() -- BlueHubLogoTXT.RainbowBlueHubTXT 
		local script = Instance.new('LocalScript', BlueHubLogoTXT)

		--Scripted by brad5203(2017)
		--Local Variables
		local hue = 0;
		local SAT = 1;
		local LUM = 1; 
		local SPEED = 1;
		--Rainbow Function
		function hsvToRgb(h, s, v)
			local r, g, b
			local i = math.floor(h * 6);
			local f = h * 6 - i;
			local p = v * (1 - s);
			local q = v * (1 - f * s);
			local t = v * (1 - (1 - f) * s);
			i = i % 6
			if i == 0 then r, g, b = v, t, p
			elseif i==1 then r, g, b=q, v, p
			elseif i==2 then r, g, b=p, v, t
			elseif i==3 then r, g, b=p, q, v
			elseif i==4 then r, g, b=t, p, v
			elseif i==5 then r, g, b=v, p, q
			end 
			return r, g, b
		end
		--Rainbow Loop 
		while (true) do
			color = hsvToRgb(hue/360, SAT, LUM);
			hue = (hue+SPEED)%360;
			wait()
			script.Parent.TextColor3 = Color3.fromHSV(hue/360, SAT, LUM)
		end
	end
	coroutine.wrap(BFDW_fake_script)()
	local function BAUH_fake_script() -- BlueWindow.leftAltToToggleUI 
		local script = Instance.new('LocalScript', BlueWindow)

		-- We must get the UserInputService before we can use it
		local UserInputService = game:GetService("UserInputService")

		-- A sample function providing one usage of InputBegan
		local function onInputBegan(input, gameProcessed)
			if input.UserInputType == Enum.UserInputType.Keyboard then
				if input.KeyCode == Enum.KeyCode.LeftAlt then
					if script.Parent.Enabled == true then
						script.Parent.Enabled = false
					elseif script.Parent.Enabled == false then
						script.Parent.Enabled = true
					end
				end
			end
		end

		UserInputService.InputBegan:Connect(onInputBegan)
	end
	coroutine.wrap(BAUH_fake_script)()
	
	local Window = {}
	function Window:CreateTab(tabname)
		local TabButton = Instance.new("TextButton")
		local SelectedImageTab = Instance.new("ImageLabel")
		local ContainerFor = Instance.new("Folder")

		ContainerFor.Name = tabname
		ContainerFor.Parent = Window

		TabButton.Name = tabname
		TabButton.Parent = TabWindow
		TabButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		TabButton.BackgroundTransparency = 1.000
		TabButton.BorderSizePixel = 0
		TabButton.Size = UDim2.new(0, 200, 0, 50)
		TabButton.Font = Enum.Font.GothamSemibold
		TabButton.Text = tabname
		TabButton.TextColor3 = Color3.fromRGB(230, 230, 230)
		TabButton.TextSize = 14.000
		TabButton.MouseButton1Click:Connect(function()
			for i, v in pairs(ContainerFor:GetChildren()) do
				if v:IsA("Frame") then
					v.Visible = true
				end
			end
		end)

		SelectedImageTab.Name = "SelectedImageTab"
		SelectedImageTab.Parent = TabButton
		SelectedImageTab.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		SelectedImageTab.BackgroundTransparency = 1.000
		SelectedImageTab.BorderSizePixel = 0
		SelectedImageTab.Position = UDim2.new(0.142857149, 0, 0.933333337, 0)
		SelectedImageTab.Size = UDim2.new(0, 100, 0, 2)
		SelectedImageTab.Image = "rbxassetid://7549716848"
		SelectedImageTab.ImageTransparency = 1.000

		-- Scripts:

		local function TIJRA_fake_script() -- TabButton.TabButtonScript 
			local script = Instance.new('LocalScript', TabButton)

			script.Parent.MouseButton1Click:Connect(function()
				for i, Image in pairs(script.Parent.Parent:GetDescendants()) do
					if Image:IsA("ImageLabel") then
						Image.ImageTransparency = 1
					end
				end
				script.Parent.SelectedImageTab.ImageTransparency = 0
				for i, contain in pairs(script.Parent.Parent.Parent.Parent.Window:GetChildren()) do
					if contain:IsA("Folder") then
						for _, Vis in pairs(contain:GetChildren()) do
							if Vis:IsA("Frame") then
								Vis.Visible = false
							end
						end
					end
				end
			end)
		end
		coroutine.wrap(TIJRA_fake_script)()
		
		local Tab = {}
		
		function Tab:CreateSection()
			local Container = Instance.new("Frame")
			local ContainerUIGridLayout = Instance.new("UIGridLayout")
			
			Container.Name = "Container"
			Container.Parent = ContainerFor
			Container.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
			Container.BorderSizePixel = 0
			Container.Position = UDim2.new(0.01236476, 0, 0.0181818176, 0)
			Container.Size = UDim2.new(0, 300, 0, 530)

			ContainerUIGridLayout.Name = "ContainerUIGridLayout"
			ContainerUIGridLayout.Parent = Container
			ContainerUIGridLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
			ContainerUIGridLayout.SortOrder = Enum.SortOrder.LayoutOrder
			ContainerUIGridLayout.CellSize = UDim2.new(0, 280, 0, 20)
			
			local Section = {}
			function Section:CreateButton(buttonname)

				local ButtonFrame = Instance.new("Frame")
				local Button = Instance.new("TextButton")
				local ButtonUICorner = Instance.new("UICorner")

				ButtonFrame.Name = "ButtonFrame"
				ButtonFrame.Parent = Container
				ButtonFrame.BackgroundColor3 = Color3.fromRGB(43, 90, 177)
				ButtonFrame.Size = UDim2.new(0, 100, 0, 100)

				Button.Name = "Button"
				Button.Parent = ButtonFrame
				Button.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
				Button.BackgroundTransparency = 1.000
				Button.BorderSizePixel = 0
				Button.Size = UDim2.new(0, 280, 0, 20)
				Button.Font = Enum.Font.GothamSemibold
				Button.Text = buttonname
				Button.TextColor3 = Color3.fromRGB(230, 230, 230)
				Button.TextSize = 14.000

				ButtonUICorner.CornerRadius = UDim.new(0, 4)
				ButtonUICorner.Name = "ButtonUICorner"
				ButtonUICorner.Parent = ButtonFrame

				-- Scripts:

				local function ILKEOI_fake_script() -- Button.ButtonScript 
					local script = Instance.new('LocalScript', Button)

					script.Parent.MouseButton1Click:Connect(function()
						script.Parent:TweenPosition(UDim2.new(0, 0,0, 0),"InOut","Quad",0.2,true)
					end)
				end
				coroutine.wrap(ILKEOI_fake_script)()

			end
			return Section
		end
		return Tab
	end
	return Window
end

return Library
